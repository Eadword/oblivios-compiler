#macro1   -34 ; This is a comment

#macro2 23;more junk messages
;this has some meaning...
 #macro3      AX ;doubt this is useful info
#macro4 macro1; Can't think of more garbage than this...

								; op   dst,src || opcode md rout e... || he x.
								;================================================
label1:   movw ax, macro1		; movw ax, -34 || 000010 00 1010 0110 || 08 A6 FF DE
datalbl:        DAT 0x00000000	; dat 0		   || 0000 0000 0000 0000 || 00 00
add macro3, bx					; add ax, bx   || 000111 00 0111 0110 || 1C 76
movw bx, datalbl				; movw bx, %-4 || 000010 01 1010 0111 || 09 A7 FF FC
label2: label3:
jmp label1						; jmp %-12     || 010111 10 1111 1111 || 5E FF FF F4
inc %[datalbl]
