#macro1   -34 ; This is a comment

#macro2 23;more junk messages
;this has some meaning...
 #macro3      AX ;doubt this is useful info
#macro4 macro1; Can't think of more garbage than this...

								; op   arg1,arg2|| opcode md arg2 arg1 || he x.
								;====================================================
label1:   movw ax, macro1		; movw ax, -34  || 000011 00 1010 0110 || 0C A6 FF DE
datalbl:        DAT 0x00000000	; dat 0		    || 0000 0000 0000 0000 || 00 00
add macro3, bx					; add ax, bx    || 001000 00 0111 0110 || 20 76
movw bx, %[datalbl]				; movw bx,%[-4] || 000011 01 1110 0111 || 0D E7 FF FC
label2: label3:
jmp %label1						; jmp %-12      || 011000 10 0000 1001 || 62 09 FF F4
inc %[datalbl]					; inc %[-12]    || 010101 10 0000 1101 || 56 0D FF F4
